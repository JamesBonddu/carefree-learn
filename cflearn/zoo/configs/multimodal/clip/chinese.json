{
  "pipeline": "cv",
  "model_name": "clip",
  "model_config": {
    "latent_dim": 768,
    "vision_latent_dim": 1024,
    "vision_patch_size": 14,
    "vision_num_layers": 24,
    "vision_num_heads": 16,
    "vocab_size": 21128,
    "context_length": 512,
    "use_text_triu_attn_mask": false,
    "text_latent_dim": 1024,
    "token_type_size": 2,
    "use_text_embedding_norm": true,
    "text_embedding_dropout": 0.1,
    "text_dropout": 0.1,
    "text_num_heads": 16,
    "text_num_layers": 24,
    "text_norm_position": "post_norm",
    "text_norm_eps": 1e-12,
    "text_feedforward_activation": "gelu",
    "text_head_pooler": "bert"
  },
  "allow_no_loss": true
}
